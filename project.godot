; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Traffair"
config/version="0.1"
run/main_scene="res://levels/main_menu.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
boot_splash/image="res://art/bootscreen.png"
config/icon="res://art/logo.png"

[autoload]

Logger="*res://logic/log.gd"
Global="*res://logic/global.gd"
BrightnessControl="*res://levels/brightness_control.tscn"
Settings="*res://logic/settings.gd"
Math="*res://logic/math.gd"

[debug]

file_logging/enable_file_logging=true
file_logging/log_path="user://godot_logs/godot.log"

[display]

window/size/viewport_width=2560
window/size/viewport_height=1440
window/size/mode=3
window/size/resizable=false
